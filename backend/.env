# Backend configuration
APP_ENV=dev
CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000,http://172.16.4.121:3000,https://127.0.0.1:3000,https://172.16.4.121:3000,https://dashboard.aman.ps
SECRET_KEY=change-me

# Local DuckDB store path
DUCKDB_PATH=.data/local.test.duckdb
DUCKDB_USER_SCOPED_TABLES=1
# Optional Postgres DSN (format: postgresql+psycopg://user:pass@host:port/db)
POSTGRES_DSN=

# Optional Supabase Postgres DSN (format: postgresql+psycopg://user:pass@host:port/db)
SUPABASE_POSTGRES_DSN=


QUERY_RATE_PER_SEC=4
QUERY_BURST=12
HEAVY_QUERY_CONCURRENCY=8
QUERY_MAX_LIMIT=10000
# Optional for multi-process shared throttling
REDIS_URL=redis://localhost:6379/0
REDIS_PREFIX=ratelimit


# Optional environment variables
# DUCKDB_THREADS: number of threads (e.g., 8).
# DUCKDB_MEMORY_LIMIT: memory limit string (e.g., 1GB, 512MB).
# DUCKDB_MEM_MB: alternative to specify MB as integer (e.g., 1024).
# DUCKDB_TEMP_DIR: directory for spill files (e.g., /tmp/duckdb).
DUCKDB_THREADS=16
DUCKDB_MEMORY_LIMIT=8GB
# DUCKDB_MEM_MB=8192
# DUCKDB_TEMP_DIR=.data/tmp

WEB_CONCURRENCY=1
HOT_RELOAD=1

APP_VERSION=0.1
UPDATES_ENABLED=1
UPDATE_REPO_OWNER=Mr-7mdan
UPDATE_REPO_NAME=Bayan