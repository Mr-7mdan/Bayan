# Application config (see backend/app/config.py)
APP_ENV=prod
CORS_ORIGINS=https://app.example.com,https://admin.example.com
SECRET_KEY=change-me
# Local analytical store (DuckDB) and metadata store
DUCKDB_PATH=/var/lib/reporting/local.duckdb
METADATA_DB_PATH=/var/lib/reporting/meta.sqlite
# Frontend/Backend URLs used by snapshot service
FRONTEND_BASE_URL=https://app.example.com
BACKEND_BASE_URL=https://api.example.com/api
SNAPSHOT_ACTOR_ID=dev_user
# Optional external DSNs (uncomment and set)
# POSTGRES_DSN=postgresql+psycopg://user:pass@host:5432/db
# SUPABASE_POSTGRES_DSN=postgresql+psycopg://user:pass@host:5432/db

# Waitress server settings consumed by run_prod_waitress.sh
RUN_SCHEDULER=1
HOST=127.0.0.1
PORT=8000
THREADS=8
CONN_LIMIT=1024
BACKLOG=2048
CHANNEL_TIMEOUT=120
IDENT=reporting-api
AI_CONCURRENCY=2
AI_TIMEOUT_SECONDS=30
